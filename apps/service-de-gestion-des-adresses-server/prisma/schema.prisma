datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Adresse {
  createdAt           DateTime  @default(now())
  dateDeCreation      DateTime?
  derniReModification DateTime?
  description         String?
  id                  String    @id @default(cuid())
  identifiant         String?
  instructions        String?
  pointDeRepere       String?
  updatedAt           DateTime  @updatedAt
  utilisateurId       String?
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}
